@model IEnumerable<AdminLteMvc.Puesto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observaciones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ubicacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Codigo_Puesto_Externo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observaciones)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ubicacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo_Puesto_Externo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Codigo_Puesto }) |
            @Html.ActionLink("Details", "Details", new { id=item.Codigo_Puesto }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Codigo_Puesto })
        </td>
    </tr>
}

</table>
